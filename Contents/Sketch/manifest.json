{
  "name": "Sketch Legend Plugin",
  "identifier": "com.amimagid.legend-plugin",
  "version": "1.1.0",
  "description": "a Sketch legend for symbols ",
  "authorEmail": "amimagid@gmail.com",
  "author": "Ami Magid",
  "commands": [
    {
      "script": "index.js",
      "handler": "runAddLegendsForSelected",
      "shortcut": "command shift y",
      "name": "Add legends for selected",
      "identifier": "com.amimagid.legend-plugin.add-selected"
    },
    {
      "script": "index.js",
      "handler": "runCleanUpLegendsForSelected",
      "shortcut": "command shift d",
      "name": "Cleanup legends for selected",
      "identifier": "com.amimagid.legend-plugin.cleanup-selected"
    },
    {
      "script": "index.js",
      "handler": "runAddLegends",
      "name": "Add legends for all",
      "identifier": "com.amimagid.legend-plugin.add"
    },
    {
      "script": "index.js",
      "handler": "runCleanUpLegends",
      "name": "Cleanup legends for all",
      "identifier": "com.amimagid.legend-plugin.cleanup"
    }

  ]
}
